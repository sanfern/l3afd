                "id": {
                    "type": "integer"
                },
                "is_plugin": {
                    "type": "boolean"
                },
                "map_args": {
                    "$ref": "#/definitions/models.L3afDNFArgs"
                },
                "map_name": {
                    "type": "string"
                },
                "memory": {
                    "type": "integer"
                },
                "monitor_maps": {
                    "type": "array",
